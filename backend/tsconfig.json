{
    "compilerOptions": {
      "target": "es2016",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "experimentalDecorators": true,
      "strictPropertyInitialization": false,
    },
    "include": ["src"],
    "exclude": ["node_modules"],
  }